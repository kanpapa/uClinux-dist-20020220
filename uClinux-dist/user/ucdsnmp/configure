echo $ac_n "checking for authentication support""... $ac_c" 1>&6
echo "configure:4541: checking for authentication support" >&5
if test "x$ac_cv_lib_crypto_EVP_md5" != "xyes" -o "x$ac_cv_header_openssl_hmac_h" != "xyes" -o "x$ac_cv_header_openssl_hmac_h" != "xyes"; then
    cat >> confdefs.h <<\EOF
#define USE_INTERNAL_MD5 1
EOF

    echo "$ac_t""Internal MD5 Support" 1>&6
    if test "x$tryopenssl" = "xyes"; then
	echo "configure: warning: Asked to use OpenSSL but I couldn't find it." 1>&2
    fi
else
    if test "x$tryopenssl" = "xno"; then
	cat >> confdefs.h <<\EOF
#define USE_INTERNAL_MD5 1
EOF

	echo "$ac_t""Internal MD5 Support" 1>&6
    else
	cat >> confdefs.h <<\EOF
#define USE_OPENSSL 1
EOF

	echo "$ac_t""OpenSSL Support" 1>&6
    fi
fi

